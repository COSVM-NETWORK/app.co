{
  "presets": ["next/babel"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ],
    "@babel/plugin-transform-regenerator",
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./components",
          "@containers": "./containers",
          "@stores": "./stores",
          "@pages": "./pages",
          "@common": "./common",
          "@utils": "./utils"
        }
      }
    ]
  ]
}
